        </createIndex>

        <addForeignKeyConstraint baseColumnNames="REGION_ID" baseTableName="BOOKSTORE_FULFILLMENT_CENTER"
                                 constraintName="FK_BOOKSTORE_FULFILLMENT_CENTER_ON_REGION" referencedColumnNames="ID"
